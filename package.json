{
  "name": "circles-api",
  "version": "0.2.4",
  "description": "Offchain database and API service for Circles",
  "main": "src/index.js",
  "private": true,
  "contributors": [
    "adzialocha"
  ],
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/CirclesUBI/circles-api.git"
  },
  "scripts": {
    "build": "babel ./src --out-dir ./build",
    "clear": "rimraf ./build",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "lint": "eslint --ignore-path .gitignore .",
    "serve": "nodemon --watch ./src --exec cross-env NODE_ENV=development babel-node ./src/index.js --source-maps inline",
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "test": "jest --forceExit --detectOpenHandles",
    "test:watch": "npm run test -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "^1.0.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "nock": "^13.0.2",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.2.0",
    "supertest": "^4.0.2",
    "truffle": "^5.1.34"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "@circles/core": "^0.11.1",
    "@circles/transfer": "^0.1.0",
    "body-parser": "^1.19.0",
    "celebrate": "^12.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.23.3",
    "http-status": "^1.4.2",
    "isomorphic-fetch": "^2.2.1",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.3.0",
    "sequelize": "^6.3.3",
    "web3": "^1.2.9",
    "winston": "^3.3.3"
  }
}
